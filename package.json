{
	"name": "voice-to-voice",
	"version": "1.0.0",
	"description": "A web-app which converts the user's voice into another voice of their choosing",
	"main": "main.js",
	"scripts": {
		"test": "echo test",
		"rmdist:web": "rm -rf dist",
		"rmdist:electron": "rm -rf electron_app/app/dist",
		"rmdist": "npm run rmdist:web && npm run rmdist:electron",
		"rmout": "rm -rf electron_app/out && rm -rf cpp/build",
		"start:electron:web": "cd electron_app && DEV_MODE=true WEB_MODE=true npm run startdev",
		"start:electron:dev": "cd electron_app && DEV_MODE=true npm run startdev",
		"start:electron": "cd electron_app && npm run start",
		"start:react:dev": "concurrently \"webpack serve --mode development --config webpack.web.js\" \"open http://localhost:3000\"",
		"package:electron": "cd electron_app && npm run package",
		"build:react": "npm run rmdist && webpack --mode production",
		"build:react:dev": "npm run rmdist && webpack --mode development",
		"build:react:electron": "npm run rmdist:electron && webpack --mode production --config webpack.electron.js",
		"build:cpp:electron:dev": "cd cpp && node-gyp rebuild --target=25.5.0 --dist-url=https://electronjs.org/headers --debug",
		"build:cpp:electron": "cd cpp && node-gyp rebuild --target=25.5.0 --dist-url=https://electronjs.org/headers --release",
		"build:electron": "npm run build:react:electron && cd electron_app && npm run make",
		"prepare:cpp": "cd cpp && node-gyp configure",
		"prepare:electron:dev": "npm run prepare:cpp && npm run build:cpp:electron:dev && npm run build:react:electron",
		"clean": "npm run rmdist && npm run rmout"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/rnash-01/voice-to-voice.git"
	},
	"keywords": [
		"AI",
		"Speech",
		"Synthesis"
	],
	"author": "Raphael Nash",
	"license": "GPL-3.0-only",
	"bugs": {
		"url": "https://github.com/rnash-01/voice-to-voice/issues"
	},
	"homepage": "https://github.com/rnash-01/voice-to-voice#readme",
	"dependencies": {
		"electron": "^25.5.0",
		"express": "^4.18.2"
	},
	"devDependencies": {
		"@babel/core": "^7.22.10",
		"@babel/preset-env": "^7.22.10",
		"@babel/preset-react": "^7.22.5",
		"@electron/rebuild": "^3.3.0",
		"babel-loader": "^9.1.3",
		"concurrently": "^8.2.0",
		"css-loader": "^6.8.1",
		"html-loader": "^4.2.0",
		"html-webpack-plugin": "^5.5.3",
		"http-server": "^14.1.1",
		"nodemon": "^3.0.1",
		"style-loader": "^3.3.3",
		"webpack": "^5.88.2",
		"webpack-cli": "^5.1.4",
		"webpack-dev-server": "^4.15.1",
		"webpack-merge": "^5.9.0"
	}
}
